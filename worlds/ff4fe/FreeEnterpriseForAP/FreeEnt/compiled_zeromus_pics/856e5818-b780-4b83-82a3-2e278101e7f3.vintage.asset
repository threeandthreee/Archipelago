text(monster name $C9) {ZeromusH}

text(monster name $CA) {D}


msfpatch {
    // hack code that draws enemy quantity to instead draw
    // extra chars for Z
    .addr $02a78e
        jml $=Zeromus__NameExtension

    .new
    Zeromus__NameExtension:
        // A = monster type at this point

        // replicate original check
        cmp #$ff
        beq $+NoEnemy

        // check for Z
        cmp #$c9
        beq $+IsZeromus

        // otherwise return
        jml $02a792

    %NoEnemy:
        jml $02a7a4

    %IsZeromus:
        // write last two characters of name instead of other behavior        
        lda $0e9e50
        dey
        dey
        sta ($34),y
        iny
        lda $36
        sta ($34),y
        iny
        lda $0e9e51
        jml $02a49f
}

